# -*- shell-script -*-

m4_include(../common.m4)

#
# Tests variants of the instruction names:
# - case insensitivity
# - aliases
#
# (Note: this test could be split into several tests.)
#

application:file-name-resolver
name:instruction-name-variants
home:.

setup.install:M4_TEST_DIR/testdata

test.run.cmdline:M4_EXECUTABLE --relative-file-argument-location testdata/instruction-name-variants.list

check.exitcode:0
check.stdout.file.eq:output/instruction-name-variants.txt
