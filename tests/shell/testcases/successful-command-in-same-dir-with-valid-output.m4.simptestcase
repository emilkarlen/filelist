# -*- shell-script -*-

#
# Tests executing a shell command.
# The shell command is executed from the file-list on the command line.
# The shell command exists with successful status and outputs valid
# file-names.
#

m4_include(common.m4)

application:file-name-lister
name:successful-command-in-same-dir-with-valid-output
home:..

setup.install:data

test.run.cmdline:M4_EXECUTABLE data/successful-command-in-same-dir-with-valid-output.list

check.exitcode:0
check.stdout.file.eq:output/successful-command-in-same-dir-with-valid-output.txt
