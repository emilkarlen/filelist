# -*- shell-script -*-

#
# Tests that filter-on-tags is handled correctly by the shell instruction:
# A filter that excludes files from the shell command.
#

m4_include(common.m4)

name:tags-filter-excluded
home:..

M4_SETUP_INSTALL_DATA_AND_INPUT(tags.list)

test.run.cmdline:M4_EXECUTABLE --filter-tags undefined-tag data/tags.list

check.exitcode:0
check.stdout.empty
