# -*- shell-script -*-

#
# Tests that filter-on-tags is handled correctly by the shell instruction:
# A filter that includes files from the shell command.
#

m4_include(common.m4)

name:tags-filter-included
home:..

setup.install:data

test.run.cmdline:M4_EXECUTABLE --filter-tags tag1 data/tags.list

check.exitcode:0
check.stdout.file.eq:output/tags-filter-included.txt
