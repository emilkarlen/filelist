# -*- shell-script -*-

#
# WHEN The file argument to @include is a directory,
# THEN the program
# SHOULD exit with error status.
#

m4_include(common.m4)

name:include-existing-dir
home:..

M4_SETUP_INSTALL_DATA_AND_INPUT(existing-dir.list)

test.run.cmdline:M4_EXECUTABLE data/existing-dir.list

# TODO is there a better exit code?
check.exitcode:M4_EXIT_FILE_DOES_NOT_EXIST
check.stdout.empty
