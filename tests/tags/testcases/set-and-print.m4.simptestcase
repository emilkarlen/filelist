# -*- shell-script -*-

#
# WHEN tags are set using the set command, and
# then the tags print command is issued
# THEN the current tags
# SHOULD be printed on a single line in alphanumerical order.
#

m4_include(common.m4)

name:tags-set-and-print
home:..

setup.install:data/set-and-print.list

test.run.cmdline:M4_EXECUTABLE set-and-print.list

check.exitcode:0
check.stdout.file.eq:output/set-and-print.txt
