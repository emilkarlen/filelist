# -*- shell-script -*-

#
# WHEN the @list is used with the option to not import tags
# THEN modifications of the tags in the included file
# SHOULD be ignored by the including file.
#

m4_include(common.m4)

name:include-option-to-not-import-tags
home:..

M4_SETUP_INSTALL_DATA_AND_INPUT(include-option-to-not-import-tags.list)

test.run.cmdline:M4_EXECUTABLE --append-tags data/include-option-to-not-import-tags.list

check.exitcode:0
check.stdout.file.eq:output/include-option-to-not-import-tags.txt
