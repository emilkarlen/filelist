# -*- shell-script -*-

#
# WHEN the tags print command
# is used together with the option of supression of non-path output
# THEN the print command
# SHOULD produced no output
#

m4_include(common.m4)

name:tags-print--suppress-non-path-output
home:..

setup.install:data/print-prefix-suffix.list

test.run.cmdline:M4_EXECUTABLE --suppress-non-path-output print-prefix-suffix.list

check.exitcode:0
check.stdout.empty
