# -*- shell-script -*-

#
# WHEN the tags print command is issued with options for prefix and suffix
# THEN the current tags
# SHOULD be printed on a single line in alphanumerical order, whith the
# given prefix and suffix.
#

m4_include(common.m4)

name:tags-print-prefix-suffix
home:..

setup.install:data/print-prefix-suffix.list

test.run.cmdline:M4_EXECUTABLE print-prefix-suffix.list

check.exitcode:0
check.stdout.file.eq:output/print-prefix-suffix.txt
