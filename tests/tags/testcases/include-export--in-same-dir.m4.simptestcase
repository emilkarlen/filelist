# -*- shell-script -*-

#
# Tests that tags are exported to included files.
#
# The included file is located in the SAME directory.
#

m4_include(common.m4)

name:include-export--in-same-dir
home:..

M4_SETUP_INSTALL_DATA_AND_INPUT(include-export--in-same-dir.list)

test.run.cmdline:M4_EXECUTABLE --append-tags data/include-export--in-same-dir.list

check.exitcode:0
check.stdout.file.eq:output/include-export--in-same-dir.txt
