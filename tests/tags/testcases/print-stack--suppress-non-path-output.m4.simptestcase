# -*- shell-script -*-

#
# WHEN the tags print-stack command
# is used together with the option of supression of non-path output
# THEN the print-stack command
# SHOULD produced no output
#

m4_include(common.m4)

name:tags-print-stack--suppress-non-path-output
home:..

setup.install:data/print-stack.list

test.run.cmdline:M4_EXECUTABLE --suppress-non-path-output print-stack.list

check.exitcode:0
check.stdout.empty
