# -*- shell-script -*-

#
# WHEN the @list is used with the option to not export tags
# THEN modifications of the current tags in the included file
# SHOULD be ignored by the included file.
#

m4_include(common.m4)

name:include-option-to-not-export-tags--current-tags--in-other-dir
home:..

M4_SETUP_INSTALL_DATA_AND_INPUT(include-option-to-not-export-tags--current-tags--in-other-dir.list)

test.run.cmdline:M4_EXECUTABLE --append-tags data/include-option-to-not-export-tags--current-tags--in-other-dir.list

check.exitcode:0
check.stdout.file.eq:output/include-option-to-not-export-tags--current-tags--in-other-dir.txt
