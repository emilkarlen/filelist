# -*- shell-script -*-

#
# WHEN the tags print-stack command
# THEN the tags stack
# SHOULD be printed on a single line
#

m4_include(common.m4)

name:tags-print-stack
home:..

setup.install:data/print-stack.list

test.run.cmdline:M4_EXECUTABLE print-stack.list

check.exitcode:0
check.stdout.file.eq:output/print-stack.txt
