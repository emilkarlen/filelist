# -*- shell-script -*-

#
# WHEN the @list is used with the option to not export tags
# THEN modifications of the current tags in the included file
# SHOULD be ignored by the included file.
#

m4_include(common.m4)

name:include-option-to-not-export-tags--current-tags
home:..

setup.install:data

test.run.cmdline:M4_EXECUTABLE --append-tags data/include-option-to-not-export-tags--current-tags.list

check.exitcode:0
check.stdout.file.eq:output/include-option-to-not-export-tags--current-tags.txt
