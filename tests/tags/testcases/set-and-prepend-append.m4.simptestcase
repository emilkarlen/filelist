# -*- shell-script -*-

#
# Tests setting tags and including them in the output
# by both prepending and appending them.
#

m4_include(common.m4)

name:set-and-prepend-append
home:..

setup.install:data

test.run.cmdline:M4_EXECUTABLE --prepend-tags --append-tags data/set-tags-and-files.list

check.exitcode:0
check.stdout.file.eq:output/set-and-prepend-append.txt
