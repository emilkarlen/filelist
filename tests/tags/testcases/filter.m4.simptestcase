# -*- shell-script -*-

#
# When a tags filter is used, then only files that are
# tagged with any of the given tags should be output.
#

m4_include(common.m4)

name:tags-filter
home:..

setup.install:data

test.run.cmdline:M4_EXECUTABLE --filter-tags included1,included2 data/filter.list

check.exitcode:0
check.stdout.file.eq:output/filter.txt
