# -*- shell-script -*-

#
# WHEN exclusion patterns (shell-pattern) are given
# THEN no file who's name matches the pattern
# SHOULD be printed
#

m4_include(common.m4)

name:file-name-condition--exclude--shell-pattern
home:..

setup.install:data/pattern-and-types

test.run.cmdline:M4_EXECUTABLE_REL_LIST pattern-and-types/file-name-condition--exclude--shell-pattern.list | sort

check.exitcode:0
check.stdout.file.eq:output/file-name-condition--exclude--shell-pattern.txt
