# -*- shell-script -*-

#
# WHEN exclusion patterns (regex) are given
# THEN no file who's name matches the pattern
# SHOULD be printed
#

m4_include(common.m4)

name:name--exclude--regex
home:..

M4_SETUP_INSTALL_DATA_AND_INPUT(name--exclude--regex.list)

test.run.cmdline:M4_EXECUTABLE_REL_LIST data/name--exclude--regex.list

check.exitcode:0
check.stdout.file.eq:output/name--exclude--regex.txt
