# -*- shell-script -*-

# Tests @list with combination of wildcard and reg-ex
# for the condition on file names.

m4_include(common.m4)

name:name--include--wildcard-and-reg-ex
home:..

setup.install:data

test.run.cmdline:M4_EXECUTABLE_REL_LIST data/name--include--wildcard-and-reg-ex.list | sort

check.exitcode:0
check.stdout.file.eq:output/name--include--wildcard-and-reg-ex.txt
