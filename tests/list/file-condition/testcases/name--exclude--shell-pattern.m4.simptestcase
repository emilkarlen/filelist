# -*- shell-script -*-

#
# WHEN exclusion patterns (shell-pattern) are given
# THEN no file who's name matches the pattern
# SHOULD be printed
#

m4_include(common.m4)

name:name--exclude--shell-pattern
home:..

setup.install:data

test.run.cmdline:M4_EXECUTABLE_REL_LIST data/name--exclude--shell-pattern.list | sort

check.exitcode:0
check.stdout.file.eq:output/name--exclude--shell-pattern.txt
