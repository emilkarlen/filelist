# -*- shell-script -*-

#
# Tests printing the file inclusion hierarchy, pretty layout.
# All file lists are located in the same directory.
#

m4_include(common.m4)

application:file-name-lister
name:in-same-directory
home:..

setup.install:data

test.run.cmdline:M4_EXECUTABLE --print-inclusion-hierarchy-pretty data/in-same-directory.list

check.exitcode:0
check.stdout.file.eq:output/in-same-directory-pretty.txt
