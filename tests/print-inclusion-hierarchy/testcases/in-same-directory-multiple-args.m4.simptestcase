# -*- shell-script -*-

#
# Tests printing the file inclusion hierarchy with many file arguments.
# All file lists are located in the same directory.
#

m4_include(common.m4)

application:file-name-lister
name:in-same-directory-multiple-args
home:..

setup.install:data

test.run.cmdline:M4_EXECUTABLE --print-inclusion-hierarchy data/in-same-directory.list data/in-same-directory.list

check.exitcode:0
check.stdout.file.eq:output/in-same-directory-multiple-args.txt
