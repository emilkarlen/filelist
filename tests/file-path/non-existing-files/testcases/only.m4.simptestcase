# -*- shell-script -*-

#
# WHEN mode for handling missing files is "only",
# and a file-path for a missing file is encountered,
# THEN the program
# SHOULD only the file in the output.
#

m4_include(common.m4)

name:only
home:..

setup.install:data

test.run.cmdline:M4_EXECUTABLE --missing-file-handling only data/list.list

check.exitcode:0
check.stdout.file.eq:output/only.txt
