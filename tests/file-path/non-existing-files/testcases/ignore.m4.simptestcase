# -*- shell-script -*-

m4_include(common.m4)

#
# WHEN mode for handling missing files is "ignore",
# and a file-path for a missing file is encountered,
# THEN the program
# SHOULD continue as if the line with the file-path did not exist in the list.
#

name:ignore
home:..

setup.install:data

test.run.cmdline:M4_EXECUTABLE --missing-file-handling ignore data/list.list

check.exitcode:0
check.stdout.file.eq:output/ignore.txt
