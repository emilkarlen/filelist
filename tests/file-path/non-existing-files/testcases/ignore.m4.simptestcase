# -*- shell-script -*-

m4_include(common.m4)

#
# WHEN mode for handling non-existing files is "ignore",
# and a file-path for a non-existing file is encountered,
# THEN the program
# SHOULD continue as if the line with the file-path did not exist in the list.
#

name:ignore
home:..

setup.install:data

test.run.cmdline:M4_EXECUTABLE --non-existing-file ignore data/list.list

check.exitcode:M4_EXIT_FILE_DOES_NOT_EXIST
check.stdout.file.eq:output/ignore.txt
