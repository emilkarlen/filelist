# -*- shell-script -*-

#
# WHEN stdin is used as a listfile on the command line
# THEN , in the inclusion hierarchy,
# SHOULD the file should be presented in the correct way
#
# (For the "correct way", see the expected output.)
#

m4_include(common.m4)

name:print-inclusion-hierarchy
home:..

setup.install:data

test.stdin:input/should-be-relative-cwd--other.list

test.run.cmdline:M4_EXECUTABLE --print-inclusion-hierarchy -- -

check.exitcode:0
check.stdout.file.eq:output/print-inclusion-hierarchy.txt
