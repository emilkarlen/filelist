# -*- shell-script -*-

#
# WHEN mode for handling missing files is "only",
# THEN the program
# SHOULD output only the non-existing files.
#

[conf]

m4_include(common.m4)

home = ..

[setup]

copy data

[act]

M4_EXECUTABLE --missing-file-handling only data/list.list

[assert]

exit-code == 0

stdout equals -file output/only.txt
