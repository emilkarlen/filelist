# -*- shell-script -*-

m4_include(common.m4)

#
# WHEN mode for handling missing files not specified
# THEN the mode
# SHOULD default to "fail".
#

home ..

[setup]

install data

[act]

M4_EXECUTABLE data/list.list

[assert]

exitcode M4_EXIT_FILE_DOES_NOT_EXIST
stdout   --rel-home output/fail.txt
