# -*- shell-script -*-

[conf]

m4_include(common.m4)

#
# WHEN mode for handling missing files not specified
# THEN the mode
# SHOULD default to "fail".
#

home = ..

[setup]

copy data

[act]

M4_EXECUTABLE data/list.list

[assert]

exit-code == M4_EXIT_FILE_DOES_NOT_EXIST
stdout equals -file output/fail.txt
