# -*- shell-script -*-

m4_include(common.m4)

#
# WHEN mode for handling missing files is "include",
# and a file-path for a missing file is encountered,
# THEN the program
# SHOULD include the file in the output just as if it was an
# existing file.
#

name:include
home:..

setup.install:data

test.run.cmdline:M4_EXECUTABLE --missing-file-handling include data/list.list

check.exitcode:0
check.stdout.file.eq:output/include.txt
