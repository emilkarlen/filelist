# -*- shell-script -*-

[conf]

m4_include(common.m4)

home = ..

[setup]

copy data

file data/absolute-file-paths-existing.list = <<EOF
@[EXACTLY_ACT]@/data/dir/existing-file.txt
EOF


[act]

M4_EXECUTABLE data/absolute-file-paths-existing.list

[assert]

exit-code == 0

stdout equals <<EOF
@[EXACTLY_ACT]@/data/dir/existing-file.txt
EOF
