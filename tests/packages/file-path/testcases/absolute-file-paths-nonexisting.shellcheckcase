# -*- shell-script -*-

m4_include(common.m4)

home ..

[setup]

M4_SETUP_INSTALL_DATA_AND_INPUT__sc(absolute-file-paths-nonexisting.list)
shell ex -s -c g:CURRENT_DIRECTORY:s:CURRENT_DIRECTORY:${SHELLCHECK_ACT}:g -c x data/absolute-file-paths-nonexisting.list

[act]

M4_EXECUTABLE data/absolute-file-paths-nonexisting.list

[assert]

exitcode M4_EXIT_FILE_DOES_NOT_EXIST
stdout   empty
