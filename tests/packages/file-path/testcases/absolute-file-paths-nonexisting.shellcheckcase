# -*- shell-script -*-

[conf]

m4_include(common.m4)

home = ..

[setup]

copy data

file data/absolute-file-paths-nonexisting.list = <<EOF
@[EXACTLY_ACT]@/data/dir/nonexisting-file.txt
EOF

[act]

M4_EXECUTABLE data/absolute-file-paths-nonexisting.list

[assert]

exit-code == M4_EXIT_FILE_DOES_NOT_EXIST

stdout empty
