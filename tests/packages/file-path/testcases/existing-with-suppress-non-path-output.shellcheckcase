# -*- shell-script -*-

#
# Tests the FILE instruction in combination with the
# --suppress-non-path-output option.
#

m4_include(common.m4)

home ..

[setup]

M4_SETUP_INSTALL_DATA_AND_INPUT(existing.list)

[act]

M4_EXECUTABLE --suppress-non-path-output data/existing.list

[assert]

exitcode 0
stdout   --rel-home output/existing.txt
