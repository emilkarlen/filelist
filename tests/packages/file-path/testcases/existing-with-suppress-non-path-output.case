#
# Tests the FILE instruction in combination with the
# --suppress-non-path-output option.
#

[setup]

copy data

file data/existing.list =
<<EOF
existing-file.txt
dir
EOF

[act]

filelist --suppress-non-path-output data/existing.list

[assert]

exit-code == 0

stdout equals
<<EOF
data/existing-file.txt
data/dir
EOF
