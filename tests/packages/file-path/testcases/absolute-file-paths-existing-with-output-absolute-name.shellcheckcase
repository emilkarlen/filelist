# -*- shell-script -*-

m4_include(common.m4)

home ..

[setup]

M4_SETUP_INSTALL_DATA_AND_INPUT__sc(absolute-file-paths-existing.list)
shell ex -s -c g:CURRENT_DIRECTORY:s:CURRENT_DIRECTORY:${SHELLCHECK_ACT}:g -c x data/absolute-file-paths-existing.list

[act]

M4_EXECUTABLE --absolute-paths data/absolute-file-paths-existing.list

[assert]

exitcode 0
stdout   --with-replaced-env-vars --rel-home output/absolute-file-paths-existing.txt
