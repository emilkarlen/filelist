# -*- shell-script -*-

m4_include(common.m4)

home ..

[setup]

M4_SETUP_INSTALL_DATA_AND_INPUT(absolute-file-paths-existing.list)
M4_REPLACE_WITH_SHELLCHECK_ACT_DIR(CURRENT_DIRECTORY, data/absolute-file-paths-existing.list)

[act]

M4_EXECUTABLE --absolute-paths data/absolute-file-paths-existing.list

[assert]

exitcode 0
stdout   --with-replaced-env-vars --rel-home output/absolute-file-paths-existing.txt
