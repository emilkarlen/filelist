[setup]

copy data

file data/list-with-existing-in-same-dir.list =
<<EOF
existing-file.txt
EOF

[act]

filelist data/list-with-existing-in-same-dir.list data/dir-with-listfile/list-with-existing-file.list

[assert]

exit-code == 0

stdout equals
<<EOF
data/existing-file.txt
data/dir-with-listfile/include-existing-file.txt
EOF
