# -*- shell-script -*-

#
# Tests executing a shell command.
# The shell command exists with unsuccessful status.
#

m4_include(common.m4)

name:unsuccessful-exit-command
home:..

M4_SETUP_INSTALL_DATA_AND_INPUT(unsuccessful-exit-command.list)

test.run.cmdline:M4_EXECUTABLE data/unsuccessful-exit-command.list

check.exitcode:M4_EXIT_SHELL_COMMAND_EXECUTION_ERROR
check.stdout.empty
