# -*- shell-script -*-

m4_include(common.m4)

home ..

[setup]

M4_SETUP_INSTALL_DATA_AND_INPUT(existing-in-same-dir-absolute-filename.list)

M4_REPLACE_WITH_SHELLCHECK_ACT_DIR(CURRENT_DIRECTORY,data/existing-in-same-dir-absolute-filename.list)

[act]

M4_EXECUTABLE data/existing-in-same-dir-absolute-filename.list

[assert]


exitcode 0
stdout --with-replaced-env-vars --rel-home output/existing-in-same-dir-absolute-filename.txt
