#
# Tests printing the file inclusion hierarchy with many file arguments.
# All file lists are located in the same directory.
#

[setup]

copy data

[act]

filelist --print-inclusion-hierarchy-pretty data/in-same-directory.list data/in-same-directory.list

[assert]

exit-code == 0

stdout equals -file output/in-same-directory-multiple-args-pretty.txt
