#
# Tests printing the file inclusion hierarchy.
# List files are located in different directories.
# Pretty layout.
#

[conf]

m4_include(common.m4)

[setup]

copy data

[act]

filelist --print-inclusion-hierarchy-pretty data/in-sub-directory.list

[assert]

exit-code == 0

stdout equals -file output/in-sub-directory-pretty.txt
