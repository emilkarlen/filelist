# -*- shell-script -*-

#
# Tests printing the file inclusion hierarchy.
# All file lists are located in the same directory.
#

[conf]

m4_include(common.m4)

home = ..

[setup]

copy data

[act]

M4_EXECUTABLE --print-inclusion-hierarchy data/in-same-directory.list

[assert]

exit-code == 0

stdout equals -file output/in-same-directory.txt
