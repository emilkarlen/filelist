# -*- shell-script -*-

#
# Tests printing the file inclusion hierarchy.
# All file lists are located in the same directory.
# Pretty layout.
#

m4_include(common.m4)

home ..

[setup]

install data

[act]

M4_EXECUTABLE --print-inclusion-hierarchy-pretty data/in-same-directory.list

[assert]

exitcode 0
stdout   --rel-home output/in-same-directory-pretty.txt
