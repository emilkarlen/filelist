# -*- shell-script -*-

#
# Tests printing the file inclusion hierarchy.
# List files are located in different directories.
#

[conf]

m4_include(common.m4)

home = ..

[setup]

copy data

[act]

M4_EXECUTABLE --print-inclusion-hierarchy data/in-sub-directory.list

[assert]

exit-code == 0

stdout equals -file output/in-sub-directory.txt
