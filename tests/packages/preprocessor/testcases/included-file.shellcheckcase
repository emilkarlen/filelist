# -*- shell-script -*-

#
# Tests preprocessing the list files.
# Only a top-level files.
#

[conf]

m4_include(common.m4)

home = ..

[setup]

M4_SETUP_INSTALL_DATA_AND_INPUT(included-file.list)

[act]

M4_EXECUTABLE --preprocessor M4_COMMON_SCRIPTS_DIR/add-txt-suffix-to-file-paths data/included-file.list

[assert]

exit-code == 0

stdout equals
       -file -rel-home output/included-file.txt
