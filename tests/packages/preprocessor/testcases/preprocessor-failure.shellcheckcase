# -*- shell-script -*-

#
# Tests preprocessing the list files.
# The preprocessor fails.
#

m4_include(common.m4)

name:preprocessor/preprocessor-failure
home:..

M4_SETUP_INSTALL_DATA_AND_INPUT(single-file.list)

test.run.cmdline:M4_EXECUTABLE --preprocessor M4_COMMON_SCRIPTS_DIR/not-a-program data/single-file.list

check.exitcode:M4_EXIT_PRE_PROCESSING
check.stdout.empty
