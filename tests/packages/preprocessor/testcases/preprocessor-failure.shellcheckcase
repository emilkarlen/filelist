# -*- shell-script -*-

#
# Tests preprocessing the list files.
# The preprocessor fails.
#

[conf]

m4_include(common.m4)

[setup]

M4_SETUP_INSTALL_DATA_AND_INPUT(single-file.list)

[act]

filelist --preprocessor @[COMMON_SCRIPTS_DIR]@/not-a-program data/single-file.list

[assert]

exit-code == M4_EXIT_PRE_PROCESSING

stdout empty
