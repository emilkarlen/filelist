# -*- shell-script -*-

#
# Tests preprocessing the list files.
# Only a top-level files.
#

m4_include(common.m4)

name:preprocessor/included-file
home:..

M4_SETUP_INSTALL_DATA_AND_INPUT(included-file.list)

test.run.cmdline:M4_EXECUTABLE --preprocessor M4_COMMON_SCRIPTS_DIR/add-txt-suffix-to-file-paths data/included-file.list

check.exitcode:0
check.stdout.file.eq:output/included-file.txt
