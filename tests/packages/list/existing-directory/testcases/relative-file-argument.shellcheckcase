# -*- shell-script -*-

m4_include(common.m4)

home ..

[setup]

install data

file data/the.list <<EOF
@LIST dir
EOF

[act]

M4_EXECUTABLE_REL_LIST data/the.list

[assert]

M4_SORT_STDOUT_TO_TMP_FILE(stdout-sorted.txt)

exitcode 0
contents --rel-tmp stdout-sorted.txt --rel-home output/relative-file-argument.txt
