# -*- shell-script -*-

#
# Ability to list the contents of the home directory -
# the directory where the source file is located.
#

m4_include(common.m4)

name:list-same-dir
home:..

M4_SETUP_INSTALL_DATA_AND_INPUT(list-same-dir.list)

test.run.cmdline:M4_EXECUTABLE_REL_LIST data/list-same-dir.list | sort

check.exitcode:0
check.stdout.file.eq:output/list-same-dir.txt
