# -*- shell-script -*-

#
# WHEN conditions for both exclusion and inclusion
# are given
# THEN the conditions for exclusion
# SHOULD have precedence over those for inclusion
#

m4_include(common.m4)

name:excludes-have-precedence
home:..

M4_SETUP_INSTALL_DATA_AND_INPUT(excludes-have-precedence.list)

test.run.cmdline:M4_EXECUTABLE_REL_LIST data/excludes-have-precedence.list

check.exitcode:0
check.stdout.file.eq:output/excludes-have-precedence.txt
