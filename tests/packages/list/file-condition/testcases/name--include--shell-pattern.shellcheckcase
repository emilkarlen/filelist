# -*- shell-script -*-

m4_include(common.m4)

home ..

[setup]

M4_SETUP_INSTALL_DATA_AND_INPUT(name--include--shell-pattern.list)

[act]

M4_EXECUTABLE_REL_LIST data/name--include--shell-pattern.list

[assert]

M4_SORT_STDOUT_TO_TMP_FILE(stdout-sorted.txt)

exitcode 0
contents --rel-tmp stdout-sorted.txt --rel-home output/name--include--shell-pattern.txt
