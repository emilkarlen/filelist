m4_include(common.m4)

name:name--include--shell-pattern-and-regex
home:..

M4_SETUP_INSTALL_DATA_AND_INPUT(name--include--shell-pattern-and-regex.list)

test.run.cmdline:M4_EXECUTABLE_REL_LIST data/name--include--shell-pattern-and-regex.list | sort

check.exitcode:0
