# -*- shell-script -*-

# Tests @list with combination of wildcard and reg-ex
# for the condition on file names.

m4_include(common.m4)

home ..

[setup]

M4_SETUP_INSTALL_DATA_AND_INPUT__sc(name--include--shell-pattern-and-regex.list)

[act]

M4_EXECUTABLE_REL_LIST data/name--include--shell-pattern-and-regex.list

[assert]

M4_SORT_STDOUT_TO_TMP_FILE(stdout-sorted.txt)

exitcode 0
contents --rel-tmp stdout-sorted.txt --rel-home output/name--include--shell-pattern-and-regex.txt
