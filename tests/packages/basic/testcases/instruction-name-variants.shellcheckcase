# -*- shell-script -*-

[conf]

m4_include(common.m4)

#
# Tests variants of the instruction names.
#

home = ..

[setup]

copy input/instruction-name-variants.list

[act]

M4_EXECUTABLE instruction-name-variants.list

[assert]

exit-code == 0

stdout equals
       -file -rel-home output/instruction-name-variants.txt
