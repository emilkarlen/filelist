# -*- shell-script -*-

#
# Tests the option to suppress all non-file output,
# used with the PRINT instruction.
#

[conf]

m4_include(common.m4)

home = ..

[setup]

copy input/simple-print.list

[act]

M4_EXECUTABLE --suppress-non-path-output simple-print.list

[assert]

exit-code == 0

stdout empty
