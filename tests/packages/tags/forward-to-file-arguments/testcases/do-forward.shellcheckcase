# -*- shell-script -*-

#
# WHEN the settings say that tags should be forwarded
# THEN the tags
# SHOULD be forwarded to following file arguments
#

m4_include(common.m4)

name:do-forward
home:..

setup.install:data

# test.run.cmdline:M4_EXECUTABLE  data/set-and-print-tags.list data/print-tags.list
test.run.cmdline:M4_EXECUTABLE --forward-tags data/set-and-print-tags.list data/print-tags.list

check.exitcode:0
check.stdout.file.eq:output/do-forward.txt
