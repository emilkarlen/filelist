#
# WHEN the settings say that tags should be forwarded
# THEN the tags
# SHOULD be forwarded to following file arguments
#

[conf]

m4_include(common.m4)

[setup]

copy data

[act]

filelist --forward-tags data/set-and-print-tags.list data/print-tags.list

[assert]

exit-code == 0

stdout equals -file output/do-forward.txt
