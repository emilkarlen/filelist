# -*- shell-script -*-

#
# WHEN the default settings are active
# THEN the tags
# SHOULD not be forwarded to following file arguments
#

m4_include(common.m4)

home ..

[setup]

install data

[act]

M4_EXECUTABLE data/set-and-print-tags.list data/print-tags.list

[assert]

exitcode 0
stdout   --rel-home output/do-not-forward.txt
