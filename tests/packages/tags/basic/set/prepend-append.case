#
# Tests setting tags and including them in the output
# by both prepending and appending them.
#

[setup]

M4_SETUP_INSTALL_DATA_AND_INPUT(set-tags-and-files.list)

[act]

filelist.py --prepend-tags --append-tags data/set-tags-and-files.list

[assert]

exit-code == 0
stdout equals -contents-of output/set-and-prepend-append.txt
