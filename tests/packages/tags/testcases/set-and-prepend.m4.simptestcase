# -*- shell-script -*-

#
# Tests setting tags and including them in the output by prepending them.
#

m4_include(common.m4)

name:set-and-prepend
home:..

M4_SETUP_INSTALL_DATA_AND_INPUT(set-tags-and-files.list)

test.run.cmdline:M4_EXECUTABLE --prepend-tags data/set-tags-and-files.list

check.exitcode:0
check.stdout.file.eq:output/set-and-prepend.txt
