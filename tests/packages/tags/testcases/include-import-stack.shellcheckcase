# -*- shell-script -*-

#
# Tests that the tags stack is imported from included files.
#

[conf]

m4_include(common.m4)

home = ..

[setup]

M4_SETUP_INSTALL_DATA_AND_INPUT(include-import-stack.list)

[act]

M4_EXECUTABLE data/include-import-stack.list

[assert]

exit-code == 0
stdout equals -file output/include-import-stack.txt
