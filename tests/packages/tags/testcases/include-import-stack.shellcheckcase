# -*- shell-script -*-

#
# Tests that the tags stack is imported from included files.
#

m4_include(common.m4)

name:include-import-stack
home:..

M4_SETUP_INSTALL_DATA_AND_INPUT(include-import-stack.list)

test.run.cmdline:M4_EXECUTABLE data/include-import-stack.list

check.exitcode:0
check.stdout.file.eq:output/include-import-stack.txt
