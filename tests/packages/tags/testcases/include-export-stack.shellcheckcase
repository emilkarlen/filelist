# -*- shell-script -*-

#
# Tests that the tags stack is exported to included files.
#

m4_include(common.m4)

home ..

[setup]

M4_SETUP_INSTALL_DATA_AND_INPUT(include-export-stack.list)

[act]

M4_EXECUTABLE data/include-export-stack.list

[assert]

exitcode 0
stdout   --rel-home output/include-export-stack.txt
