# -*- shell-script -*-

#
# WHEN the tags print-stack command
# THEN the tags stack
# SHOULD be printed on a single line
#

[conf]

m4_include(common.m4)

home = ..

[setup]

copy input/print-stack.list

[act]

M4_EXECUTABLE print-stack.list

[assert]

exit-code == 0
stdout equals -file output/print-stack.txt
