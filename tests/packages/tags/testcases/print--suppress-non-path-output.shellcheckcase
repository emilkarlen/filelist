# -*- shell-script -*-

#
# WHEN the tags print command
# is used together with the option of supression of non-path output
# THEN the print command
# SHOULD produced no output
#

m4_include(common.m4)

home ..

[setup]

install input/print-prefix-suffix.list

[act]

M4_EXECUTABLE --suppress-non-path-output print-prefix-suffix.list

[assert]

exitcode 0
stdout   empty
