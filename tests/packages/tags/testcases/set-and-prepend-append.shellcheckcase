# -*- shell-script -*-

#
# Tests setting tags and including them in the output
# by both prepending and appending them.
#

[conf]

m4_include(common.m4)

[setup]

M4_SETUP_INSTALL_DATA_AND_INPUT(set-tags-and-files.list)

[act]

filelist --prepend-tags --append-tags data/set-tags-and-files.list

[assert]

exit-code == 0
stdout equals -file output/set-and-prepend-append.txt
