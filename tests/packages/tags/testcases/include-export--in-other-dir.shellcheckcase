# -*- shell-script -*-

#
# Tests that tags are exported to included files.
#
# The included file is located in a DIFFERENT directory.
#

[conf]

m4_include(common.m4)

home = ..

[setup]

M4_SETUP_INSTALL_DATA_AND_INPUT(include-export--in-other-dir.list)

[act]

M4_EXECUTABLE --append-tags data/include-export--in-other-dir.list

[assert]

exit-code == 0
stdout equals -file output/include-export--in-other-dir.txt
