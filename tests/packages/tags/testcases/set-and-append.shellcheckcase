# -*- shell-script -*-

#
# Tests setting tags and including them in the output by appending them.
#

m4_include(common.m4)

home ..

[setup]

M4_SETUP_INSTALL_DATA_AND_INPUT__sc(set-tags-and-files.list)

[act]

M4_EXECUTABLE --append-tags data/set-tags-and-files.list

[assert]

exitcode 0
stdout   --rel-home output/set-and-append.txt
