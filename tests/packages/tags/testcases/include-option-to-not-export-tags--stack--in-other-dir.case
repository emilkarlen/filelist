#
# WHEN the @list is used with the option to not export tags
# THEN modifications of the tags stack in the included file
# SHOULD be ignored by the included file.
#

[conf]

m4_include(common.m4)

[setup]

M4_SETUP_INSTALL_DATA_AND_INPUT(include-option-to-not-export-tags--stack--in-other-dir.list)

[act]

filelist --append-tags data/include-option-to-not-export-tags--stack--in-other-dir.list

[assert]

exit-code == @[EXIT_TAGS_ERROR]@

stdout   empty
