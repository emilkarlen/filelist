# -*- shell-script -*-

#
# Tests preprocessing the list files.
# Only a top-level files.
#

m4_include(common.m4)

application:file-name-lister
name:preprocessor/included-file
home:..

setup.install:data

test.run.cmdline:M4_EXECUTABLE --preprocessor ${SIMPTEST_HOME}/scripts/add-txt-suffix-to-file-paths data/included-file.list

check.exitcode:0
check.stdout.file.eq:output/included-file.txt
