# -*- shell-script -*-

#
# Tests the FILE instruction in combination with the
# --suppress-non-path-output option.
#

m4_include(common.m4)

application:file-name-lister
name:file-existing-with-suppress-non-path-output
home:.

setup.install:testdata

test.run.cmdline:M4_EXECUTABLE --suppress-non-path-output testdata/file-existing.list

check.exitcode:0
check.stdout.file.eq:output/file-existing.txt
